# Frontend Production Environment Configuration for sidarsih.site
VITE_API_URL=https://sidarsih.site/api
VITE_WHATSAPP_API_URL=https://sidarsih.site/whatsapp
VITE_WHATSAPP_SOCKET_URL=https://sidarsih.site
VITE_SOCKET_URL=https://sidarsih.site
VITE_APP_DOMAIN=sidarsih.site
VITE_APP_ENV=production
VITE_ENABLE_HTTPS=true

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Security Configuration
VITE_SECURE_COOKIES=true
VITE_SAME_SITE=strict

# Performance Configuration
VITE_ENABLE_PWA=true
VITE_ENABLE_COMPRESSION=true

# Analytics Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ID=your-analytics-id

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=false
VITE_ENABLE_DEMO_MODE=false